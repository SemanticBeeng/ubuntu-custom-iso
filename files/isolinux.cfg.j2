# D-I config version 2.0
path
UI menu.c32
PROMPT 0
MENU TITLE Boot Menu
TIMEOUT 300

default install

label install
  menu label ^Autoinstall Ubuntu JeOS
  kernel /install/vmlinuz
  append language=en country=AU locale=en_AU.UTF-8 file=/cdrom/preseed/{{ preseed_file }} initrd=/install/initrd.gz auto=true priority=high --

